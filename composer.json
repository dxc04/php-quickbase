{
    "name": "rossity/php-quickbase",
    "description": "A Quick Base RESTful API implementation of their OpenAPI spec.",
    "keywords": [
        "rossity",
        "php-quickbase",
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/rossity/php-quickbase",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
          "name": "Ross Ball",
          "email": "w.ross.ball@gmail.com",
          "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Rossity\\PhpQuickbase\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Rossity\\PhpQuickbase\\Test\\" : "test/" }
    }
}
