{
  "name": "rossity/php-quickbase",
  "description": "A Quick Base RESTful API implementation of their OpenAPI spec.",
  "keywords": [
    "rossity",
    "php-quickbase",
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api"
  ],
  "homepage": "https://github.com/rossity/php-quickbase",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Ross Ball",
      "email": "w.ross.ball@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "squizlabs/php_codesniffer": "~2.6",
    "friendsofphp/php-cs-fixer": "~2.12"
  },
  "autoload": {
    "psr-4": {
      "Rossity\\PhpQuickbase\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rossity\\PhpQuickbase\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  }
}
